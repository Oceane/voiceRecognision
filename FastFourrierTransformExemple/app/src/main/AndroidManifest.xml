<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.enkaichen.fastfourriertransformexemple" >

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"></uses-permission>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>
    <uses-permission android:name="android.permission.RECORD_AUDIO"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="Password Saver"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.enkaichen.fastfourriertransformexemple.SecondRecording"
            android:label="Enter your password"
            android:screenOrientation="portrait"
            android:parentActivityName="com.example.enkaichen.fastfourriertransformexemple.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.enkaichen.fastfourriertransformexemple.MainActivity" />
        </activity>
        <activity
            android:name="com.example.enkaichen.fastfourriertransformexemple.MiddleRecording"
            android:label="Say your password again"
            android:screenOrientation="portrait"
            android:parentActivityName="com.example.enkaichen.fastfourriertransformexemple.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.enkaichen.fastfourriertransformexemple.MainActivity" />
        </activity>
        <activity
            android:name="com.example.enkaichen.fastfourriertransformexemple.Middle2Recording"
            android:label="Say your password again"
            android:screenOrientation="portrait"
            android:parentActivityName="com.example.enkaichen.fastfourriertransformexemple.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.enkaichen.fastfourriertransformexemple.MainActivity" />
        </activity>
        <activity
            android:name="com.example.enkaichen.fastfourriertransformexemple.MiddleFinalRecording"
            android:label="Say your password again"
            android:screenOrientation="portrait"
            android:parentActivityName="com.example.enkaichen.fastfourriertransformexemple.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.enkaichen.fastfourriertransformexemple.MainActivity" />
        </activity>
        <activity
            android:name="com.example.enkaichen.fastfourriertransformexemple.MyActivity"
            android:label="Say your password again"
            android:screenOrientation="portrait"
            android:parentActivityName="com.example.enkaichen.fastfourriertransformexemple.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.enkaichen.fastfourriertransformexemple.MainActivity" />
        </activity>
        <activity
            android:name=".DisplayContact2"
            android:label="Say your password again"
            android:screenOrientation="portrait"
            android:parentActivityName="com.example.enkaichen.fastfourriertransformexemple.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.enkaichen.fastfourriertransformexemple.MainActivity" />
        </activity>
    </application>

</manifest>
